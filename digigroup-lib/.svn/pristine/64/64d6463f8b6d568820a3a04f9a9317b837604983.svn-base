package uiengine
{
	public class MenuDescriptor
	{
		public var actionLabel:String;
		public var menuGeneratorClass:Class;
		public var menuLabel:String;
		public var menuGeneratorParams:Object;
		
		public function MenuDescriptor(actionLabel:String, menuGeneratorClass:Class=null, 
			menuLabel:String=null, menuGeneratorParams:Object=null)
		{
			this.actionLabel = actionLabel;
			this.menuGeneratorClass = menuGeneratorClass;
			this.menuLabel = menuLabel;
			this.menuGeneratorParams = menuGeneratorParams;
		}

	}
}