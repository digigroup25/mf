package uiengine
{
	import actions.IActionDescriptor;
	
	public class ActionDescriptor2 implements IActionDescriptor
	{
		private var _label:String;
		public var voType:Class;
		public var messageType:String;
		
		public function get label():String {
			return _label;
		}
		
		public function set label(value:String):void {
			_label = value;
		}
		
		public function ActionDescriptor2(label:String, voType:Class, messageType:String) {
			this.label = label;
			this.voType = voType;
			this.messageType = messageType;
		}
	}
}