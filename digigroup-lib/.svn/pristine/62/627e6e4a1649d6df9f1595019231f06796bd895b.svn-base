package mindmaps.elementderivatives.subway.components
{
	import flash.display.Sprite;
	
	import mx.core.UIComponent;
	
	public class SpriteComponent extends UIComponent  {
		public function SpriteComponent (sprite : Sprite)
		{
		    super ();
		
		    explicitHeight = sprite.height;
		    explicitWidth = sprite.width;
		
		    addChild (sprite);
		}
	}
	
}