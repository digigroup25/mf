package collections.tree
{
	import collections.tree.*;
	import collections.*;
	
	public class TreeRankerResult
	{
		public function get rankedTree():TreeCollectionEx {
			return _rankedTree;
		}
		
		public function get treeHeight():int {
			return _treeHeight;
		}
		
		public function get maxRank():int {
			return _maxRank;
		}
		
		public function get minRank():int {
			return _minRank;
		}
		
		public function TreeRankerResult(rankedTree:TreeCollectionEx, treeHeight:int, 
			maxRank:int, minRank:int) {
			this._rankedTree = rankedTree;
			this._treeHeight = treeHeight;
			this._maxRank = maxRank;
			this._minRank = minRank;	
		}
		
		private var _rankedTree:TreeCollectionEx;
		private var _treeHeight:int;
		private var _maxRank:int;
		private var _minRank:int;
	}
}