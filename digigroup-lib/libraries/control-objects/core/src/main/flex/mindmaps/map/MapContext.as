package mindmaps.map {
	import mx.collections.ArrayCollection;

	/**
	 * MapContext holds all objects necessary to provide contextual info about the map - e.g.
	 * node types (Task, Appointment), actions available for each type (Prioritize, New),
	 * data presentation filters (ViewDataFilter), etc.
	 **/
	public class MapContext {

		public function MapContext(nodeTypes:Array) {
			this.nodeTypes = nodeTypes;
		}

		public var nodeTypes:Array;
	}
}
