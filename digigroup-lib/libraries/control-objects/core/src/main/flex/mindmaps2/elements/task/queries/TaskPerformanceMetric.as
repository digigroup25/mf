package mindmaps2.elements.task.queries {

	[Bindable]
	public class TaskPerformanceMetric {
		public var expectedCommittedPoints:uint = 0;

		public var actualCommittedPoints:uint = 0;

		public var expectedUncommittedPoints:uint = 0;

		public var actualUncommittedPoints:uint = 0;


		public var expectedCommittedHours:Number = 0;

		public var actualCommittedHours:Number = 0;

		public var expectedUncommittedHours:Number = 0;

		public var actualUncommittedHours:Number = 0;

		public function get actualHours():Number {
			return actualCommittedHours + actualUncommittedHours;
		}
	}
}
